<Window x:Class="YP_2.MinCount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YP_2"
        mc:Ignorable="d"
        Title="Редактирование" Height="130" Width="250" Icon="Resources/Черновик.ico">
    <Grid>
        <StackPanel>
            <TextBlock>Введите новое минимальное значение</TextBlock>
            <TextBox Name="TbNewMinCount" Width="100" Margin="0,7"></TextBox>
            <Button Name="OkBtn" Width="50" Click="OkBtn_Click">OK</Button>
        </StackPanel>
    </Grid>
</Window>
